          <a class="d-flex align-items-center text-start mx-3 me-0 pb-3" data-bs-toggle="pill" href="#tab-5">
            <h6 class="mt-n1 mb-0">Semi Private</h6>
          </a>
        </li>
      </ul>
      <div class="tab-content">
        <div id="tab-1" class="tab-pane fade show p-0 active">
          <% if(colleges.length>0 ){%> <%colleges.forEach(college=>{%>

          <div class="job-item p-4 mb-4">
            <div class="row g-4">
              <div class="col-sm-12 col-md-8 d-flex align-items-center">

                <div class="text-start ps-4">
                  <h5 class="mb-3">
                    <%= college.name%>
                  </h5>
                  <span class="text-truncate me-3"><i class="fa fa-map-marker-alt custom-blue-i me-2"></i>
                    <%= college.city%> , <%= college.country %>
                  </span>
                  <span class="text-truncate me-3"><i class="fas fa-percentage custom-blue-i me-2"></i>
                    <%= college.minimum_percent%>
                  </span>
                  <span class="text-truncate me-0"><i class="fas fa-university custom-blue-i me-2"></i>
                    <%= college.type%>
                  </span>
                </div>
              </div>
              <div class="col-sm-12 col-md-4 d-flex flex-column align-items-start align-items-md-end justify-content-center">
                <div class="d-flex mb-3">

                  <a class="btn custom-blue" href="/college-detail/<%= college._id%>">Apply Now</a>
                </div>
                <small class="text-truncate"><i class="far fa-calendar-alt custom-blue-i me-2"></i>Date
                  Line: <%= college.deadline%></small>
              </div>
            </div>
          </div>
          <%})}%>


        </div>
